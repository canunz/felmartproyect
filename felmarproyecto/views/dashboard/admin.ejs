<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Panel Administrador - Felmart</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    
    <style>
        html, body {
            margin: 0;
            padding: 0;
            width: 100%;
            box-sizing: border-box;
        }
        body {
            background-color: var(--light-bg);
            font-family: 'Inter', sans-serif;
            line-height: 1.6;
            min-height: 100vh;
        }
        .navbar-admin {
            background: #fff;
            box-shadow: none;
            position: sticky;
            top: 0;
            z-index: 1000;
            width: 100%;
            margin: 0;
            padding: 0;
            display: flex;
            align-items: center;
            justify-content: space-between;
            min-height: 70px;
            border-radius: 0;
        }
        .navbar-admin .logo-section {
            display: flex;
            align-items: center;
            margin-left: 32px;
        }
        .navbar-admin .logo-section img {
            height: 40px;
            width: auto;
            margin-right: 12px;
        }
        .navbar-admin .logo-section .logo-text {
            font-size: 2rem;
            font-weight: 700;
            color: #00616e;
        }
        .navbar-admin .admin-menu {
            display: flex;
            align-items: center;
            gap: 32px;
            flex: 1;
            justify-content: center;
        }
        .navbar-admin .admin-menu a {
            color: #6c757d;
            font-weight: 500;
            font-size: 1.1rem;
            text-decoration: none;
            display: flex;
            align-items: center;
            gap: 8px;
            padding: 8px 16px;
            border-radius: 8px;
            transition: background 0.2s;
        }
        .navbar-admin .admin-menu a.active, .navbar-admin .admin-menu a:hover {
            background: #e9f7fa;
            color: #00616e;
        }
        .navbar-admin .user-section {
            margin-right: 32px;
        }
        @media (max-width: 1200px) {
            .navbar-admin .admin-menu {
                gap: 16px;
            }
            .navbar-admin .logo-section {
                margin-left: 12px;
            }
            .navbar-admin .user-section {
                margin-right: 12px;
            }
        }
        @media (max-width: 900px) {
            .navbar-admin .admin-menu {
                flex-wrap: wrap;
                gap: 8px;
            }
            .navbar-admin .logo-section .logo-text {
                font-size: 1.3rem;
            }
        }
        :root {
            /* Navigation colors - Exactos del cliente.ejs */
            --nav-primary-color: #00616e;
            --nav-primary-dark: #004d57;
            --nav-primary-light: #00818f;
            --nav-secondary-color: #6c757d;
            --nav-white: #ffffff;
            --nav-shadow: 0 4px 20px rgba(0,0,0,0.08);
            --nav-transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
            
            /* Dashboard colors */
            --success-color: #28a745;
            --warning-color: #ffc107;
            --danger-color: #dc3545;
            --info-color: #17a2b8;
            --purple-color: #6f42c1;
            --orange-color: #fd7e14;
            --light-bg: #f8f9fa;
            --border-radius: 12px;
            --shadow: 0 2px 12px rgba(0,0,0,0.08);
            --shadow-hover: 0 4px 20px rgba(0,0,0,0.12);
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Inter', sans-serif;
            background-color: var(--light-bg);
            line-height: 1.6;
            background: #f8f9fa;
            margin: 0;
            padding: 0;
        }

        /* Header Navigation - Exacto del cliente.ejs */
        .header-nav { padding: 0.35rem 0; }
        .nav-container { display: flex; align-items: center; justify-content: flex-start; padding-left: 2px; padding-right: 0; max-width: 100%; margin: 0; gap: 4px; }
        .nav-container > * { margin-right: 4px; }
        .nav-container > *:last-child { margin-right: 0; }
        .logo-icon { width: 22px; height: 22px; font-size: 0.8rem; margin-right: 6px; }
        .logo-section span { font-size: 0.92rem; }
        .nav-link, .nav-link i { font-size: 0.82rem !important; }
        .user-btn { font-size: 0.82rem !important; padding: 0.32rem 0.8rem !important; border-radius: 1rem !important; }

        .nav-menu {
            display: flex;
            align-items: center;
            gap: 0;
            margin: 0;
            padding: 0;
            list-style: none;
        }

        .nav-item {
            display: flex;
            align-items: center;
        }

        .nav-link {
            display: flex;
            align-items: center;
            gap: 8px;
            padding: 0.5rem 1rem;
            color: var(--nav-secondary-color);
            text-decoration: none;
            border-radius: 8px;
            transition: var(--nav-transition);
            font-weight: 500;
            position: relative;
        }

        .nav-link:hover,
        .nav-link.active {
            color: var(--nav-primary-color);
            background: rgba(0, 97, 110, 0.05);
        }

        .nav-link i {
            font-size: 16px;
        }

        .user-dropdown {
            position: relative;
        }

        .user-btn {
            background: linear-gradient(135deg, var(--nav-primary-color), var(--nav-primary-light));
            color: white;
            border: none;
            padding: 0.5rem 1rem;
            border-radius: 50px;
            display: flex;
            align-items: center;
            gap: 8px;
            font-weight: 500;
            cursor: pointer;
            transition: var(--nav-transition);
        }

        .user-btn:hover {
            background: linear-gradient(135deg, var(--nav-primary-dark), var(--nav-primary-color));
        }

        .dropdown-menu {
            position: absolute;
            top: calc(100% + 8px);
            right: 0;
            background: var(--nav-white);
            border: none;
            border-radius: 12px;
            box-shadow: 0 10px 40px rgba(0, 0, 0, 0.1);
            min-width: 200px;
            opacity: 0;
            visibility: hidden;
            transform: translateY(-10px);
            transition: all 0.2s;
            z-index: 1000;
            padding: 0.5rem;
            margin-top: 0.5rem;
        }

        .dropdown-menu.show {
            opacity: 1;
            visibility: visible;
            transform: translateY(0);
        }

        .dropdown-item {
            display: flex;
            align-items: center;
            gap: 8px;
            padding: 0.75rem 1rem;
            color: var(--nav-primary-color);
            text-decoration: none;
            transition: var(--nav-transition);
            font-size: 14px;
            border-radius: 8px;
            margin: 0.25rem 0;
            font-weight: 500;
        }

        .dropdown-item:hover {
            background: rgba(0, 97, 110, 0.05);
            color: var(--nav-primary-color);
            transform: translateX(5px);
        }

        .dropdown-item.text-danger {
            color: var(--danger-color) !important;
        }

        .dropdown-item.text-danger:hover {
            background: rgba(220, 53, 69, 0.1);
            color: var(--danger-color) !important;
        }

        .dropdown-item i {
            width: 20px;
            text-align: center;
        }

        /* Dashboard Content */
        .dashboard-container {
            padding: 2.5rem 0;
            padding-top: 110px;
            min-height: calc(100vh - 80px);
        }

        .page-header {
            margin-bottom: 3rem;
            text-align: center;
        }

        .page-title {
            font-size: 2.5rem;
            font-weight: 700;
            color: var(--nav-primary-color);
            margin-bottom: 0.5rem;
        }

        .page-subtitle {
            color: var(--nav-secondary-color);
            font-size: 1.2rem;
            margin: 0;
        }

        /* Alert Bar */
        .alert-bar {
            background: linear-gradient(135deg, #fff3cd, #ffeaa7);
            border: 1px solid #ffc107;
            border-radius: var(--border-radius);
            padding: 1rem 1.5rem;
            margin-bottom: 2rem;
            display: flex;
            align-items: center;
            gap: 1rem;
        }

        .alert-bar i {
            color: #856404;
            font-size: 1.2rem;
        }

        .alert-bar-text {
            flex: 1;
            color: #856404;
            font-weight: 500;
        }

        /* Statistics Cards */
        .stats-grid {
            margin-bottom: 3rem;
        }

        .stats-card {
            background: white;
            border-radius: var(--border-radius);
            padding: 2rem;
            box-shadow: var(--shadow);
            transition: var(--nav-transition);
            border: none;
            height: 100%;
            cursor: pointer;
            overflow: hidden;
            position: relative;
            margin-bottom: 1.5rem;
        }

        .stats-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            height: 4px;
        }

        .stats-card:hover {
            transform: translateY(-4px);
            box-shadow: var(--shadow-hover);
        }

        .stats-card.primary::before {
            background: linear-gradient(90deg, var(--nav-primary-color), var(--nav-primary-light));
        }

        .stats-card.warning::before {
            background: linear-gradient(90deg, var(--warning-color), #ffcd3c);
        }

        .stats-card.success::before {
            background: linear-gradient(90deg, var(--success-color), #34ce57);
        }

        .stats-card.info::before {
            background: linear-gradient(90deg, var(--info-color), #20c997);
        }

        .stats-card.danger::before {
            background: linear-gradient(90deg, var(--danger-color), #f56565);
        }

        .stats-card.purple::before {
            background: linear-gradient(90deg, var(--purple-color), #8b5cf6);
        }

        .stats-card.orange::before {
            background: linear-gradient(90deg, var(--orange-color), #fb923c);
        }

        .card-content {
            display: flex;
            align-items: center;
            justify-content: space-between;
        }

        .card-info h6 {
            font-size: 0.9rem;
            color: var(--nav-secondary-color);
            margin-bottom: 0.5rem;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.5px;
        }

        .card-info h2 {
            font-size: 2.2rem;
            font-weight: 700;
            margin: 0;
            line-height: 1;
        }

        .card-info h2.primary { color: var(--nav-primary-color); }
        .card-info h2.warning { color: var(--warning-color); }
        .card-info h2.success { color: var(--success-color); }
        .card-info h2.info { color: var(--info-color); }
        .card-info h2.danger { color: var(--danger-color); }
        .card-info h2.purple { color: var(--purple-color); }
        .card-info h2.orange { color: var(--orange-color); }

        .card-icon {
            font-size: 2.5rem;
            opacity: 0.8;
        }

        .card-icon.primary { color: var(--nav-primary-color); }
        .card-icon.warning { color: var(--warning-color); }
        .card-icon.success { color: var(--success-color); }
        .card-icon.info { color: var(--info-color); }
        .card-icon.danger { color: var(--danger-color); }
        .card-icon.purple { color: var(--purple-color); }
        .card-icon.orange { color: var(--orange-color); }

        /* Quick Actions */
        .action-section {
            background: white;
            border-radius: var(--border-radius);
            padding: 2.5rem;
            box-shadow: var(--shadow);
            margin-bottom: 3rem;
        }

        .section-title {
            font-size: 1.4rem;
            font-weight: 600;
            color: var(--nav-primary-color);
            margin-bottom: 2rem;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .action-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 1.5rem;
        }

        .action-btn {
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 1.5rem;
            border-radius: var(--border-radius);
            text-decoration: none;
            font-weight: 600;
            font-size: 1rem;
            transition: var(--nav-transition);
            border: none;
            cursor: pointer;
            text-align: center;
            color: white;
            gap: 0.75rem;
        }

        .action-btn i {
            font-size: 1.2rem;
        }

        .action-btn.primary {
            background: linear-gradient(135deg, var(--nav-primary-color), var(--nav-primary-light));
            box-shadow: 0 4px 15px rgba(0, 97, 110, 0.2);
        }

        .action-btn.primary:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 20px rgba(0, 97, 110, 0.3);
            color: white;
        }

        .action-btn.warning {
            background: linear-gradient(135deg, var(--warning-color), #ffcd3c);
            box-shadow: 0 4px 15px rgba(255, 193, 7, 0.2);
        }

        .action-btn.warning:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 20px rgba(255, 193, 7, 0.3);
            color: white;
        }

        .action-btn.info {
            background: linear-gradient(135deg, var(--info-color), #20c997);
            box-shadow: 0 4px 15px rgba(23, 162, 184, 0.2);
        }

        .action-btn.info:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 20px rgba(23, 162, 184, 0.3);
            color: white;
        }

        .action-btn.success {
            background: linear-gradient(135deg, var(--success-color), #34ce57);
            box-shadow: 0 4px 15px rgba(40, 167, 69, 0.2);
        }

        .action-btn.success:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 20px rgba(40, 167, 69, 0.3);
            color: white;
        }

        .action-btn.purple {
            background: linear-gradient(135deg, var(--purple-color), #8b5cf6);
            box-shadow: 0 4px 15px rgba(111, 66, 193, 0.2);
        }

        .action-btn.purple:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 20px rgba(111, 66, 193, 0.3);
            color: white;
        }

        .action-btn.orange {
            background: linear-gradient(135deg, var(--orange-color), #fb923c);
            box-shadow: 0 4px 15px rgba(253, 126, 20, 0.2);
        }

        .action-btn.orange:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 20px rgba(253, 126, 20, 0.3);
            color: white;
        }

        /* Chart Cards */
        .charts-grid {
            margin-bottom: 3rem;
        }

        .chart-card {
            background: white;
            border-radius: var(--border-radius);
            box-shadow: var(--shadow);
            margin-bottom: 2rem;
            overflow: hidden;
            transition: var(--nav-transition);
            border: 1px solid #e9ecef;
        }

        .chart-card:hover {
            transform: translateY(-2px);
            box-shadow: var(--shadow-hover);
        }

        .chart-card .card-header {
            background: linear-gradient(135deg, #f8f9fa, #e9ecef);
            border-bottom: 1px solid #e9ecef;
            padding: 1.5rem;
        }

        .chart-card .card-body {
            padding: 2rem;
            height: 350px;
            position: relative;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .chart-card .card-title {
            font-size: 1.2rem;
            font-weight: 600;
            color: var(--nav-primary-color);
            margin: 0;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .chart-card .card-title i {
            font-size: 1.1rem;
            opacity: 0.8;
        }

        /* Mejorar el espaciado de los gráficos */
        .charts-grid .row {
            margin: 0 -0.75rem;
        }

        .charts-grid .col-lg-6,
        .charts-grid .col-xl-4,
        .charts-grid .col-xl-12 {
            padding: 0 0.75rem;
        }

        /* Responsive para gráficos */
        @media (max-width: 1200px) {
            .chart-card .card-body {
                height: 300px;
            }
        }

        @media (max-width: 768px) {
            .chart-card .card-body {
                height: 250px;
                padding: 1.5rem;
            }
            
            .chart-card .card-header {
                padding: 1rem;
            }
            
            .chart-card .card-title {
                font-size: 1.1rem;
            }
        }

        /* Table Styles */
        .table-section {
            background: white;
            border-radius: var(--border-radius);
            box-shadow: var(--shadow);
            overflow: hidden;
            margin-bottom: 2rem;
        }

        .table-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 1.5rem 2rem;
            border-bottom: 1px solid #e9ecef;
            background: linear-gradient(135deg, #f8f9fa, #e9ecef);
        }

        .table-title {
            font-size: 1.2rem;
            font-weight: 600;
            color: var(--nav-primary-color);
            margin: 0;
            display: flex;
            align-items: center;
            gap: 0.5rem;
        }

        .btn-outline-custom {
            background: transparent;
            border: 2px solid var(--nav-primary-color);
            color: var(--nav-primary-color);
            padding: 0.5rem 1rem;
            border-radius: var(--border-radius);
            text-decoration: none;
            font-weight: 500;
            font-size: 0.9rem;
            transition: var(--nav-transition);
        }

        .btn-outline-custom:hover {
            background: var(--nav-primary-color);
            color: white;
        }

        .table-custom {
            margin: 0;
        }

        .table-custom thead th {
            background-color: #f8f9fa;
            border-top: none;
            border-bottom: 2px solid #e9ecef;
            padding: 1rem 1.5rem;
            font-weight: 600;
            color: var(--nav-primary-color);
            font-size: 0.85rem;
            text-transform: uppercase;
            letter-spacing: 0.5px;
        }

        .table-custom tbody td {
            padding: 1rem 1.5rem;
            vertical-align: middle;
            border-bottom: 1px solid #f1f3f4;
        }

        .table-custom tbody tr:hover {
            background-color: #f8f9fa;
        }

        .status-badge {
            padding: 0.4rem 0.8rem;
            border-radius: 20px;
            font-size: 0.75rem;
            font-weight: 500;
            text-transform: capitalize;
        }

        .status-badge.pendiente {
            background-color: #fff3cd;
            color: #856404;
        }

        .status-badge.cotizada {
            background-color: #cce5ff;
            color: #004085;
        }

        .status-badge.programada {
            background-color: #d1ecf1;
            color: #0c5460;
        }

        .status-badge.completada {
            background-color: #d4edda;
            color: #155724;
        }

        .status-badge.cancelada {
            background-color: #f8d7da;
            color: #721c24;
        }

        .status-badge.activo {
            background-color: #d4edda;
            color: #155724;
        }

        .status-badge.inactivo {
            background-color: #f8d7da;
            color: #721c24;
        }

        .btn-action {
            background: var(--info-color);
            color: white;
            padding: 0.4rem 0.8rem;
            border-radius: 6px;
            text-decoration: none;
            font-size: 0.8rem;
            font-weight: 500;
            transition: var(--nav-transition);
            border: none;
        }

        .btn-action:hover {
            background: #138496;
            color: white;
            transform: translateY(-1px);
        }

        /* Priority Alerts */
        .priority-alerts {
            background: white;
            border-radius: var(--border-radius);
            box-shadow: var(--shadow);
            padding: 2rem;
            margin-bottom: 3rem;
        }

        .alert-item {
            display: flex;
            align-items: center;
            gap: 1rem;
            padding: 1rem;
            border-radius: var(--border-radius);
            margin-bottom: 1rem;
            border-left: 4px solid;
        }

        .alert-item.high {
            background: rgba(220, 53, 69, 0.1);
            border-left-color: var(--danger-color);
        }

        .alert-item.medium {
            background: rgba(255, 193, 7, 0.1);
            border-left-color: var(--warning-color);
        }

        .alert-item.low {
            background: rgba(23, 162, 184, 0.1);
            border-left-color: var(--info-color);
        }

        .alert-icon {
            font-size: 1.2rem;
        }

        .alert-icon.high { color: var(--danger-color); }
        .alert-icon.medium { color: var(--warning-color); }
        .alert-icon.low { color: var(--info-color); }

        .alert-content {
            flex: 1;
        }

        .alert-title {
            font-weight: 600;
            margin-bottom: 0.25rem;
        }

        .alert-description {
            font-size: 0.9rem;
            color: var(--nav-secondary-color);
        }

        /* Animations */
        @keyframes fadeInUp {
            from {
                opacity: 0;
                transform: translateY(20px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        .fade-in-up {
            animation: fadeInUp 0.6s ease-out;
        }

        .stagger-1 { animation-delay: 0.1s; }
        .stagger-2 { animation-delay: 0.15s; }
        .stagger-3 { animation-delay: 0.2s; }
        .stagger-4 { animation-delay: 0.25s; }
        .stagger-5 { animation-delay: 0.3s; }
        .stagger-6 { animation-delay: 0.35s; }
        .stagger-7 { animation-delay: 0.4s; }

        /* Responsive Design */
        @media (max-width: 768px) {
            .nav-menu {
                display: none;
            }

            .dashboard-container {
                padding: 1.5rem 0;
            }

            .page-title {
                font-size: 2rem;
            }

            .stats-card {
                margin-bottom: 1rem;
            }

            .card-content {
                flex-direction: column;
                text-align: center;
                gap: 1rem;
            }

            .table-header {
                flex-direction: column;
                gap: 1rem;
                text-align: center;
            }

            .action-grid {
                grid-template-columns: 1fr;
            }

            .charts-grid .row > div {
                margin-bottom: 2rem;
            }
        }

        @media (max-width: 992px) {
            .nav-menu { display: none !important; }
            .admin-dropdown-btn { display: flex !important; }
        }
        @media (min-width: 993px) {
            .nav-menu { display: flex !important; }
            .admin-dropdown-menu { position: absolute; }
        }
    </style>
    <script>
    // Plugin para mostrar texto en el centro de los charts
    Chart.register({
        id: 'centerText',
        afterDraw: function(chart) {
            if (chart.config.options.centerText) {
                const ctx = chart.ctx;
                const centerText = chart.config.options.centerText.text;
                const fontSize = chart.config.options.centerText.fontSize || 24;
                const color = chart.config.options.centerText.color || '#333';
                const meta = chart.getDatasetMeta(0);
                if (!meta || !meta.data || !meta.data[0]) return;
                const x = meta.data[0].x;
                const y = meta.data[0].y;
                ctx.save();
                ctx.font = `bold ${fontSize}px Inter, Arial, sans-serif`;
                ctx.textAlign = 'center';
                ctx.textBaseline = 'middle';
                ctx.fillStyle = color;
                ctx.fillText(centerText, x, y);
                ctx.restore();
            }
        }
    });
    </script>
</head>
