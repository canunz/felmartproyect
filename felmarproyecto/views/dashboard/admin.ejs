    <!-- Custom JavaScript -->
    <script>
        // Leer los datos de los scripts JSON
        const clientesComunaLabels = JSON.parse(document.getElementById('clientesComunaLabels').textContent);
        const clientesComunaData = JSON.parse(document.getElementById('clientesComunaData').textContent);
        const cotizacionesEstadoLabels = JSON.parse(document.getElementById('cotizacionesEstadoLabels').textContent);
        const cotizacionesEstadoData = JSON.parse(document.getElementById('cotizacionesEstadoData').textContent);
        const solicitudesEstadoLabels = JSON.parse(document.getElementById('solicitudesEstadoLabels').textContent);
        const solicitudesEstadoData = JSON.parse(document.getElementById('solicitudesEstadoData').textContent);
        const totalClientes = clientesComunaData.reduce((a, b) => a + b, 0);
        const totalCotizaciones = cotizacionesEstadoData.reduce((a, b) => a + b, 0);

        // Charts initialization
        document.addEventListener('DOMContentLoaded', function() {
            // Clientes por Comuna Chart (Pie)
            const ctxComuna = document.getElementById('clientesPorComunaChart');
            if (ctxComuna) {
                new Chart(ctxComuna, {
                    type: 'pie',
                    data: {
                        labels: clientesComunaLabels,
                        datasets: [{
                            data: clientesComunaData,
                            backgroundColor: [
                                '#00616e', '#17a2b8', '#28a745', '#fd7e14', '#ffc107', '#6f42c1', '#dc3545', '#20c997', '#8b5cf6', '#fb923c'
                            ]
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        plugins: {
                            legend: {
                                position: 'bottom',
                                labels: { padding: 15, usePointStyle: true, font: { size: 11 } }
                            }
                        },
                        centerText: {
                            text: totalClientes.toString(),
                            fontSize: 28,
                            color: '#00616e'
                        }
                    }
                });
            }

            // Solicitudes por Estado Chart (Bar)
            const ctxResiduos = document.getElementById('tiposResiduosChart');
            if (ctxResiduos) {
                new Chart(ctxResiduos, {
                    type: 'bar',
                    data: {
                        labels: solicitudesEstadoLabels,
                        datasets: [{
                            data: solicitudesEstadoData,
                            backgroundColor: ['#dc3545', '#ffc107', '#28a745', '#6f42c1'],
                            borderRadius: 4
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        plugins: { legend: { display: false } },
                        scales: {
                            y: { beginAtZero: true },
                            x: { grid: { display: false } }
                        }
                    }
                });
            }

            // Poblar tabla de solicitudes por estado
            const solicitudesTable = document.getElementById('solicitudesEstadoTable');
            if (solicitudesTable && solicitudesEstadoLabels.length > 0) {
                const colors = ['#dc3545', '#ffc107', '#28a745', '#6f42c1'];
                solicitudesTable.innerHTML = solicitudesEstadoLabels.map((label, index) => `
                    <tr>
                        <td>
                            <div class="d-flex align-items-center">
                                <div class="me-2" style="width: 8px; height: 8px; border-radius: 50%; background-color: ${colors[index % colors.length]};"></div>
                                <span style="font-size: 0.85rem; font-weight: 500; text-transform: capitalize;">${label}</span>
                            </div>
                        </td>
                        <td class="text-end">
                            <span style="font-size: 0.85rem; font-weight: 600; color: #00616e;">${solicitudesEstadoData[index]}</span>
                        </td>
                    </tr>
                `).join('');
            }

            // Cotizaciones por Estado Chart (Doughnut)
            const ctxCotizaciones = document.getElementById('cotizacionesPorEstadoChart');
            if (ctxCotizaciones) {
                new Chart(ctxCotizaciones, {
                    type: 'doughnut',
                    data: {
                        labels: cotizacionesEstadoLabels,
                        datasets: [{
                            data: cotizacionesEstadoData,
                            backgroundColor: [
                                '#ffc107', // pendiente
                                '#28a745', // aceptada
                                '#dc3545', // rechazada
                                '#6c757d'  // vencida
                            ],
                            borderWidth: 0,
                            cutout: '60%'
                        }]
                    },
                    options: {
                        responsive: true,
                        maintainAspectRatio: false,
                        plugins: {
                            legend: {
                                position: 'bottom',
                                labels: {
                                    padding: 20,
                                    usePointStyle: true,
                                    font: { size: 12 }
                                }
                            }
                        },
                        centerText: {
                            text: totalCotizaciones.toString(),
                            fontSize: 28,
                            color: '#00616e'
                        }
                    }
                });
            }

            // Animation on page load
            const animatedElements = document.querySelectorAll('.fade-in-up');
            animatedElements.forEach((element, index) => {
                element.style.opacity = '0';
                element.style.transform = 'translateY(20px)';
                
                setTimeout(() => {
                    element.style.transition = 'all 0.6s ease-out';
                    element.style.opacity = '1';
                    element.style.transform = 'translateY(0)';
                }, index * 50);
            });
        });

        // Add click effect to stat cards
        document.querySelectorAll('.stats-card').forEach(card => {
            card.addEventListener('click', function() {
                this.style.transform = 'scale(0.98)';
                setTimeout(() => {
                    this.style.transform = '';
                }, 150);
            });
        });

        // Real-time updates simulation
        function updateDashboard() {
            console.log('Actualizando dashboard administrativo...');
            // Aquí solo actualizaremos cuando haya datos reales del servidor
            // No se realizarán cambios aleatorios
        }

        // Update dashboard every 30 seconds
        setInterval(updateDashboard, 30000);

        // Auto-refresh functionality
        let autoRefreshInterval;
        
        function startAutoRefresh() {
            autoRefreshInterval = setInterval(() => {
                updateDashboard();
            }, 60000); // Every minute
        }

        function stopAutoRefresh() {
            if (autoRefreshInterval) {
                clearInterval(autoRefreshInterval);
            }
        }

        // Start auto-refresh on page load
        startAutoRefresh();

        // Stop auto-refresh when page is hidden
        document.addEventListener('visibilitychange', function() {
            if (document.hidden) {
                stopAutoRefresh();
            } else {
                startAutoRefresh();
            }
        });

        // Keyboard shortcuts for admin efficiency
        document.addEventListener('keydown', function(e) {
            // Ctrl/Cmd + N for new solicitud
            if ((e.ctrlKey || e.metaKey) && e.key === 'n') {
                e.preventDefault();
                window.location.href = '/admin/solicitudes/crear';
            }
            
            // Ctrl/Cmd + K for quick search
            if ((e.ctrlKey || e.metaKey) && e.key === 'k') {
                e.preventDefault();
                // Focus search input or open search modal
                console.log('Quick search activated');
            }

            // Ctrl/Cmd + D for dashboard refresh
            if ((e.ctrlKey || e.metaKey) && e.key === 'd') {
                e.preventDefault();
                updateDashboard();
                showNotification('Dashboard actualizado', 'success');
            }
        });

        // Emergency alert system
        function checkEmergencyAlerts() {
            // Simulate checking for emergency situations
            const emergencyTypes = [
                'Derrame de químicos reportado',
                'Vehículo de transporte con falla mecánica',
                'Cliente reporta incidencia con residuos',
                'Operador requiere asistencia inmediata'
            ];
            
            if (Math.random() > 0.95) { // 5% chance of emergency
                const emergencyType = emergencyTypes[Math.floor(Math.random() * emergencyTypes.length)];
                showEmergencyAlert(emergencyType);
            }
        }

        function showEmergencyAlert(message) {
            const alert = document.createElement('div');
            alert.className = 'alert alert-danger alert-dismissible fade show position-fixed';
            alert.style.cssText = 'top: 20px; left: 50%; transform: translateX(-50%); z-index: 9999; min-width: 400px; border: 2px solid #dc3545;';
            alert.innerHTML = `
                <div class="d-flex align-items-center">
                    <i class="fas fa-exclamation-triangle fa-2x me-3 text-danger"></i>
                    <div>
                        <h6 class="mb-1"><strong>¡ALERTA DE EMERGENCIA!</strong></h6>
                        <p class="mb-0">${message}</p>
                    </div>
                </div>
                <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
            `;
            
            document.body.appendChild(alert);
            
            // Play alert sound (if available)
            try {
                const audio = new Audio('data:audio/wav;base64,');
                audio.play();
            } catch (e) {
                // Audio not supported or failed to load
            }
            
            setTimeout(() => {
                if (alert.parentNode) {
                    alert.remove();
                }
            }, 10000);
        }

        // Check for emergency alerts every 2 minutes
        setInterval(checkEmergencyAlerts, 120000);

        // Welcome message on first load
        window.addEventListener('load', function() {
            setTimeout(() => {
                showNotification('¡Bienvenido al Panel de Administración de Felmart!', 'success');
            }, 1000);
        });

        // Notification system
        function showNotification(message, type = 'info') {
            const notification = document.createElement('div');
            notification.className = `alert alert-${type} alert-dismissible fade show position-fixed`;
            notification.style.cssText = 'top: 20px; right: 20px; z-index: 9999; min-width: 300px;';
            notification.innerHTML = `
                <i class="fas ${type === 'success' ? 'fa-check-circle' : 'fa-info-circle'} me-2"></i>
                ${message}
                <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
            `;
            
            document.body.appendChild(notification);
            
            setTimeout(() => {
                if (notification.parentNode) {
                    notification.remove();
                }
            }, 5000);
        }

        // Initialize tooltips
        var tooltipTriggerList = [].slice.call(document.querySelectorAll('[data-bs-toggle="tooltip"]'));
        var tooltipList = tooltipTriggerList.map(function (tooltipTriggerEl) {
            return new bootstrap.Tooltip(tooltipTriggerEl);
        });
    </script>
